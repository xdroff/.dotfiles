# color bash
export CLICOLOR=1
export LSCOLORS=GxFxCxDxBxegedabagaced

# Show git branch
parse_git_branch() {
     git branch 2> /dev/null | sed -e '/^[^*]/d' -e 's/* \(.*\)/ (\1)/'
}

# PS1
#export PS1="\[\033[33m\]\w\[\033[32m\]\$(parse_git_branch)\[\033[92m\] $\[\033[00m\] "
export PS1="[\u@\h] \[\033[33m\]\w\[\033[32m\]\$(parse_git_branch)\[\033[92m\] $\[\033[00m\] "
